{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cypress:delete-reports": "rmdir mochawesome-report || true",
    "cypress:open": "npx cypress open",
    "cypress:open:prod": "SET ENV=prod & npx cypress open",
    "cypress:allure:run": "npx cypress run && allure generate --clean && allure open",
    "cypress:allure:run:prod": "SET ENV=prod & npm run cypress:allure:run",
    "cypress:mochawesome:run": "node .\\scripts\\cypress.js",
    "cypress:mochawesome:run:prod": "SET ENV=prod & npm run cypress:mochawesome:run",
    "cypress:all:run": "npm run cypress:delete-reports & npm run cypress:mochawesome:run & allure generate --clean && allure open",
    "cypress:all:run:prod": "SET ENV=prod & npm run cypress:all:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0-next.0",
    "@angular/common": "^14.0.0-next.0",
    "@angular/compiler": "^14.0.0-next.0",
    "@angular/core": "^14.0.0-next.0",
    "@angular/forms": "^14.0.0-next.0",
    "@angular/platform-browser": "^14.0.0-next.0",
    "@angular/platform-browser-dynamic": "^14.0.0-next.0",
    "@angular/router": "^14.0.0-next.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.0",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.0.0-next.0",
    "@shelex/cypress-allure-plugin": "^2.30.2",
    "@types/jasmine": "~4.0.0",
    "allure-commandline": "^2.18.1",
    "cypress": "^10.6.0",
    "cypress-teamcity-reporter": "^3.0.0",
    "jasmine-core": "~4.3.0",
    "junit": "^1.4.9",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "~4.7.2"
  }
}
